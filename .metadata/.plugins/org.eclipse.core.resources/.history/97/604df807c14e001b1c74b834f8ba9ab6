package Arrays;

public class adding2arrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	int num=5;
	int a[] = new int [5];
	int b[] = new int[5];
	int c[] = new int[5];
	for(int i=0;i<5;i++) {
		a[i]=i;
	}
	for(int i=0;i<5;i++) {
	b[i]=num;
	++num;
	} 
	for(int i=0;i<5;i++) {
		System.out.println(a[i] +" " );
		
	}
	System.out.println();
	for(int i=0;i<5;i++) { 
		System.out.println(b[i]+" ");
	}
	}
}
